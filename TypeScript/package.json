{
  "name": "gilded-rose-ts",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "precompile": "rimraf app/**/*.js test/**/*.js",
    "compile": "tsc",
    "test": "jest"
  },
  "license": "ISC",
  "private": true,
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "jest": "^23.4.2",
    "rimraf": "~2.5.2",
    "ts-jest": "^23.1.3",
    "typescript": "^3.0.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testURL": "http://localhost",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "collectCoverage": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
